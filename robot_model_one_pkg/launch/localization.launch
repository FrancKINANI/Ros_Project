<launch>

	<arg name="custom_amcl_launch_file" default="$(find navigation_stack)/launch/amcl.launch"/>


	<include file="$(find robot_model_one_pkg)/launch/house.launch"/>

	<include file="$(arg custom_amcl_launch_file)">
		<arg name="base_link_name" value="body_link"/>
		<arg name="map_file" value="$(find robot_model_one_pkg)/../maps/aws_house/map.yaml"/>
	</include>

	<node name="rviz" pkg="rviz" type="rviz" respawn="false"
			args="-d $(find navigation_stack)/rviz/amcl.rviz"/>
</launch>